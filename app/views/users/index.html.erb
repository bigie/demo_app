<h1>Daftar user</h1>

<table>
  <tr>
    <th>name</th>
    <th>email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'show', user %></td>
    <td><%= link_to 'edit', edit_user_path(user) %></td>
    <td><%= link_to 'delete', user, :confirm => 'yakin ni mau di hapus?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<b><%= link_to 'new user', new_user_path %>
